<div id="venues-near-by" >
  <% @venues_near_by.try(:each) do |v| %>
    <h3><%= v.name %></h3>
    <p class="store-code"><%= v.store_code %></p>
    <p class="rank"><%= v.rank %></p>
    <p><%= v.address %></p>
    <p><%= v.phone %></p>
    <p><%= v.venue_hours %></p>
    <p><%= link_to v.website %></p>
    <p>Get Directions</p>
    <script>
      var map = new GMaps({
        el: '#map',
        lat: <%= session[:location][0] %>,
        lng: <%= session[:location][1] %>,
        zoom: 11
      });
      map.addMarker({
       lat: <%= v.latitude %>,
       lng: <%= v.longitude %>,
     });
    </script>
<% end %>
</div>
    <%= binding.pry %>

<script id="venues-template" type="text/x-handlebars-template">
  {{#each this}}
    <h4>{{name}}</h4>
    <p class="store-code">{{store_code}}</p>
    <p class="rank">{{rank}}</p>
    <p>{{address}}</p>
    <p>{{phone}}</p>
    <p>{{hours}}</p>
    <p><a href={{website}}>Visit Store Website</a></p>
    <p>Get Directions</p>
  {{/each}}
</script>
