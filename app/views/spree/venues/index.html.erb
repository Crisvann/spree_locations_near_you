<div class="venue-search">
  <h1>Locations</h1>
<div class="row">
  <div class="col-md-6">
    <form id="searchVenues">
      <input type="text" name="zipcode">
      <button type="submit"><i class="glyphicon glyphicon-repeat"></i></button>
    </form>
  </div>
  <div class="col-md-6">
    <label>
      <input class="rank" type="checkbox" name="sliver" value="sliver"/>sliver
    </label><span>sliver</span>
    <label>
      <input class="rank" type="checkbox" name="gold" value="gold"/>gold
    </label><span>gold</span>
    <label>
      <input class="rank" type="checkbox" name="platinum" value="platinum"/>platinum
    </label><span>platium</span>
  </div>
</div>
<div class="row">
  <div class="col-md-8">
    <%= render partial: "map" %>
  </div>
  <div class="col-md-4">
    <%= render partial: "venues_list" %>
  </div>
</div>

