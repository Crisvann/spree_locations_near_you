<% venues.each do | venue| %>
  <tr>
    <td><%= venue.name %></td>
    <td><%= venue.street_address %></td>
    <td><%= venue.city %></td>
    <td><%= venue.country %></td>
    <td><%= venue.state %></td>
    <td><%= venue.zip %></td>
    <td><%= venue.phone %></td>
    <td><%= venue.website %></td>
    <td data-hook="admin_venues_index_row_actions" class="actions actions-2 text-right">
      <%= link_to_edit venue, :no_text => true %>
      <%= link_to_delete venue, :no_text => true %>
    </td>
  </tr>
<% end %>
